            <script type="text/javascript">
                /*<![CDATA[*/
                var i = "One moment please..."
                var j = "Login"
                    /*]]>*/
                    $(window).on("pageshow", function () {
                        $(":submit").prop("disabled", false);
                        $(":submit").attr("value", j);
                    });
                $(document).ready(function () {
                    $("#fm1").submit(function () {
                        $(":submit").attr("disabled", true);
                        $(":submit").attr("value", i);
                        return true;
                    });
